<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <link rel="icon" type="image/png" sizes="32x32" href="/logoTab.png">
  <title>Casen - Phone Cases</title>
  <script src="https://cdn.tailwindcss.com"></script>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" />
  <style>
    .price-apply-btn {
      background: linear-gradient(to right, #ef4444, #b91c1c);
      color: white;
      border: none;
      border-radius: 9999px;
      padding: 0.25rem 0.75rem;
      font-size: 0.75rem;
      font-weight: 600;
      cursor: pointer;
      transition: all 0.3s ease;
      box-shadow: 0 4px 6px -1px rgba(239, 68, 68, 0.3);
    }

    .price-apply-btn:hover {
      transform: translateY(-2px);
      box-shadow: 0 6px 8px -1px rgba(239, 68, 68, 0.4);
    }

    .price-apply-btn:active {
      transform: translateY(0);
    }

    .price-input,
    .color-input {
      outline: none;
      border: 1px solid #d1d5db;
      border-radius: 0.375rem;
      padding: 0.25rem 0.5rem;
      font-size: 0.875rem;
    }

    .price-input:focus,
    .color-input:focus {
      border-color: #ef4444;
      box-shadow: 0 0 0 3px rgba(239, 68, 68, 0.2);
    }

    .offer-badge {
      background: linear-gradient(135deg, #ff416c, #ff4b2b);
      color: white;
      padding: 2px 8px;
      border-radius: 12px;
      font-weight: bold;
      font-size: 0.85rem;
      display: inline-flex;
      align-items: center;
      gap: 4px;
      box-shadow: 0 2px 4px rgba(255, 65, 108, 0.3);
    }
  </style>
  <script defer src="https://cdn.jsdelivr.net/npm/alpinejs@3.13.10/dist/cdn.min.js"></script>
</head>

<body class="bg-white font-sans flex flex-col min-h-screen">
  <!-- Header -->
  <header class="bg-white shadow-sm sticky top-0 z-50">
    <div class="container mx-auto px-4 py-3 flex items-center justify-between">
      <!-- Logo -->
      <div class="flex items-center">
        <a href="/">
          <img src="/logo.png" alt="Logo" class="h-8 object-contain">
        </a>
      </div>
      <!-- Desktop Navigation -->
      <nav class="hidden md:flex space-x-5">
        <a href="/" class="text-sm font-medium transition-opacity hover:opacity-80">Home</a>
        <a href="#" class="text-sm font-medium transition-opacity hover:opacity-80">About</a>
        <a href="/products" class="text-sm font-medium transition-opacity hover:opacity-80">Phone Cases</a>
        <a href="#" class="text-sm font-medium transition-opacity hover:opacity-80">Tablet Cases</a>
        <a href="#" class="text-sm font-medium transition-opacity hover:opacity-80">Sale</a>
      </nav>
      <!-- Icons & Auth -->
      <div class="flex items-center space-x-4">
        <!-- Wishlist -->
        <button class="p-2 rounded-full hover:bg-gray-100 transition-colors">
          <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5" fill="none" viewBox="0 0 24 24" stroke="currentColor">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
              d="M4.318 6.318a4.5 4.5 0 000 6.364L12 20.364l7.682-7.682a4.5 4.5 0 00-6.364-6.364L12 7.636l-1.318-1.318a4.5 4.5 0 00-6.364 0z" />
          </svg>
        </button>
        <!-- Cart -->
        <button class="p-2 rounded-full hover:bg-gray-100 transition-colors relative"
          onclick="window.location.href='/cart'">
          <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5" fill="none" viewBox="0 0 24 24" stroke="currentColor">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
              d="M3 3h2l.4 2M7 13h10l4-8H5.4M7 13L5.4 5M7 13l-2.293 2.293c-.63.63-.184 1.707.707 1.707H17m0 0a2 2 0 100 4 2 2 0 000-4zm-8 2a2 2 0 11-4 0 2 2 0 014 0z" />
          </svg>
        </button>
        <!-- User Menu -->
        <% if (locals.user && user.profilePic) { %>
          <div class="relative" x-data="{ open: false }" @click.away="open = false">
            <button @click="open = !open"
              class="rounded-full overflow-hidden w-8 h-8 border border-transparent hover:border-gray-300 transition-colors focus:outline-none">
              <img src="<%= user.profilePic %>" alt="Profile" class="w-full h-full object-cover" />
            </button>
            <div x-show="open" x-transition:enter="transition ease-out duration-150"
              x-transition:enter-start="opacity-0 scale-95" x-transition:enter-end="opacity-100 scale-100"
              x-transition:leave="transition ease-in duration-100" x-transition:leave-start="opacity-100 scale-100"
              x-transition:leave-end="opacity-0 scale-95"
              class="absolute right-0 mt-2 w-44 bg-white shadow-lg rounded-lg py-1 z-50">
              <a href="/profile"
                class="block px-4 py-2 text-sm text-gray-700 hover:bg-gray-50 transition-colors">Profile</a>
              <form action="/logout" method="post">
                <button type="submit"
                  class="w-full text-left px-4 py-2 text-sm text-gray-700 hover:bg-gray-50 transition-colors">Logout</button>
              </form>
            </div>
          </div>
          <% } else if (locals.user) { %>
            <div class="relative" x-data="{ open: false }" @click.away="open = false">
              <button @click="open = !open"
                class="w-8 h-8 flex items-center justify-center text-gray-600 hover:text-gray-900 transition-colors">
                <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5" fill="none" viewBox="0 0 24 24"
                  stroke="currentColor">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                    d="M16 7a4 4 0 11-8 0 4 4 0 018 0zM12 14a7 7 0 00-7 7h14a7 7 0 00-7-7z" />
                </svg>
              </button>
              <div x-show="open" x-transition:enter="transition ease-out duration-150"
                x-transition:enter-start="opacity-0 scale-95" x-transition:enter-end="opacity-100 scale-100"
                x-transition:leave="transition ease-in duration-100" x-transition:leave-start="opacity-100 scale-100"
                x-transition:leave-end="opacity-0 scale-95"
                class="absolute right-0 mt-2 w-44 bg-white shadow-lg rounded-lg py-1 z-50">
                <a href="/profile" class="block px-4 py-2 text-sm text-gray-700 hover:bg-gray-50">Profile</a>
                <form action="/logout" method="post">
                  <button type="submit"
                    class="w-full text-left px-4 py-2 text-sm text-gray-700 hover:bg-gray-50">Logout</button>
                </form>
              </div>
            </div>
            <% } else { %>
              <a href="/login" class="text-sm font-medium hover: transition-colors flex items-center">
                <svg xmlns="http://www.w3.org/2000/svg" class="h-4 w-4 mr-1" fill="none" viewBox="0 0 24 24"
                  stroke="currentColor">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                    d="M16 7a4 4 0 11-8 0 4 4 0 018 0zM12 14a7 7 0 00-7 7h14a7 7 0 00-7-7z" />
                </svg>
                Sign In
              </a>
              <% } %>
                <!-- Mobile Menu Button -->
                <button class="md:hidden text-gray-600 hover:text-gray-900">
                  <svg xmlns="http://www.w3.org/2000/svg" class="h-6 w-6" fill="none" viewBox="0 0 24 24"
                    stroke="currentColor">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 6h16M4 12h16M4 18h16" />
                  </svg>
                </button>
      </div>
    </div>
  </header>
  <!-- Main Content -->
  <main class="container mx-auto px-4 py-6 flex-grow">
    <!-- Top Filters -->
    <div class="flex flex-wrap gap-4 mb-6">
      <div class="flex gap-2">
        <input type="text" id="searchInput" placeholder="Search products..."
          class="border border-gray-300 rounded-full px-3 py-1 text-sm w-48"
          value="<%= (query && query.search) ? query.search : '' %>" />
        <button id="clearFilters" class="px-3 py-1 text-sm bg-gray-200 rounded-full hover:bg-gray-300">
          Clear
        </button>
      </div>
      <div class="ml-auto flex items-center gap-2">
        <span class="text-sm text-gray-600">Sort by:</span>
        <select id="sortSelect" class="border border-gray-300 rounded-full px-3 py-1 text-sm">
          <option value="bestselling" <%=(!query || !query.sort || query.sort==='bestselling' ) ? ' selected' : '' %>
            >Best selling</option>
          <option value="priceLowToHigh" <%=(query && query.sort==='priceLowToHigh' ) ? ' selected' : '' %>>Price: Low
            to High</option>
          <option value="priceHighToLow" <%=(query && query.sort==='priceHighToLow' ) ? ' selected' : '' %>>Price: High
            to Low</option>
          <option value="aA-zZ" <%=(query && query.sort==='aA-zZ' ) ? ' selected' : '' %>>aA - zZ</option>
          <option value="zZ-aA" <%=(query && query.sort==='zZ-aA' ) ? ' selected' : '' %>>zZ - aA</option>
          <option value="newest" <%=(query && query.sort==='newest' ) ? ' selected' : '' %>>Newest</option>
        </select>
      </div>
    </div>
    <!-- Product area -->
    <div class="flex flex-col lg:flex-row gap-6">
      <!-- Sidebar Filters -->
      <aside class="lg:w-64 bg-white p-4 rounded-lg shadow-sm">
        <!-- Price Filter -->
        <div class="mb-6">
          <h3 class="font-medium mb-3">Price</h3>
          <div class="flex gap-2 items-center">
            <input type="number" id="minPrice" placeholder="Min" class="w-20 p-2 border rounded text-sm price-input"
              min="0" value="<%= (query && query.minPrice) ? query.minPrice : '0' %>" />
            <span class="text-sm">to</span>
            <input type="number" id="maxPrice" placeholder="Max" class="w-20 p-2 border rounded text-sm price-input"
              min="0" value="<%= (query && query.maxPrice) ? query.maxPrice : '1000' %>" />
            <button id="applyPrice" class="price-apply-btn">Apply</button>
          </div>
        </div>
        <!-- Color Filter -->
        <div class="mb-6">
          <h3 class="font-medium mb-3">Color</h3>
          <div class="flex gap-2 items-center">
            <input type="text" id="colorFilterInput" placeholder="e.g. violet, black..."
              class="flex-1 p-2 border border-gray-300 rounded text-sm color-input"
              value="<%= (query && query.color) ? query.color : '' %>" />
            <button id="applyColorFilter" class="price-apply-btn" style="padding: 0.25rem 0.5rem; font-size: 0.7rem;">
              Go
            </button>
          </div>
        </div>
        <!-- Category Filter -->
        <div class="mb-6">
          <h3 class="font-medium mb-3">Category</h3>
          <div class="space-y-2">
            <% if (typeof filterCategories !=='undefined' && Array.isArray(filterCategories)) { %>
              <% for (var i=0; i < filterCategories.length; i++) { %>
                <% var cat=filterCategories[i]; var isChecked='' ; if (query && query.category) { var activeCats=(typeof
                  query.category==='string' ) ? query.category.split(',') : []; if (activeCats.includes(cat)) {
                  isChecked=' checked' ; } } %>
                  <label class="flex items-center gap-2 text-sm">
                    <input type="checkbox" class="form-checkbox h-4 w-4 text-red-600 filter-checkbox"
                      data-type="category" data-value="<%= cat %>" <%=isChecked %>
                    />
                    <%= cat %>
                  </label>
                  <% } %>
                    <% } %>
          </div>
        </div>
        <!-- Product Filter -->
        <div class="mb-6">
          <h3 class="font-medium mb-3">Products</h3>
          <div class="space-y-2">
            <% if (typeof filterProducts !=='undefined' && Array.isArray(filterProducts)) { %>
              <% for (var i=0; i < filterProducts.length; i++) { %>
                <% var prod=filterProducts[i]; var isCheckedProd='' ; if (query && query.product) { var
                  activeProds=(typeof query.product==='string' ) ? query.product.split(',') : []; if
                  (activeProds.includes(prod)) { isCheckedProd=' checked' ; } } %>
                  <label class="flex items-center gap-2 text-sm">
                    <input type="checkbox" class="form-checkbox h-4 w-4 text-red-600 filter-checkbox"
                      data-type="product" data-value="<%= prod %>" <%=isCheckedProd %>
                    />
                    <%= prod %>
                  </label>
                  <% } %>
                    <% } %>
          </div>
        </div>
      </aside>
      <!-- Product Grid -->
      <div class="flex-1">
        <% if (!products || products.length===0) { %>
          <div class="text-center py-12 text-gray-500">
            <i class="fas fa-box-open text-4xl mb-4"></i>
            <p>No products found. Try adjusting your filters.</p>
          </div>
          <% } else { %>
            <div class="grid grid-cols-1 sm:grid-cols-2 lg:grid-cols-3 xl:grid-cols-4 gap-6">
              <% for (var i=0; i < products.length; i++) { %>
                <% var p=products[i]; var productId=p.productId || 'unknown' ; var productName=p.productName
                  || 'Unnamed Product' ; var categoryName=(p.category && p.category.categoryName) ?
                  p.category.categoryName : 'Uncategorized' ; var variant=p.variants || {}; var color=variant.color
                  || 'Unknown' ; var priceNum=(typeof variant.price==='number' ) ? variant.price : 0; var
                  originalPrice=(p.offerInfo && p.offerInfo.originalPrice) ? p.offerInfo.originalPrice : priceNum; var
                  imageUrl=(variant.images && Array.isArray(variant.images) && variant.images[0]) ? variant.images[0]
                  : 'https://via.placeholder.com/300' ; %>
                  <a href="/product?product=<%= productId %>" class="block">
                    <div class="bg-white rounded-lg overflow-hidden shadow-sm hover:shadow-md transition-shadow">
                      <div class="relative">
                        <img src="<%= imageUrl %>" alt="<%= productName %> (<%= color %>)"
                          class="w-full h-64 object-contain p-2" />
                        <% if (p.offerInfo) { %>
                          <div class="absolute top-2 left-2 z-10">
                            <span class="offer-badge">
                              Save <%= p.offerInfo.discountPercentage.toFixed(0) %>%
                            </span>
                          </div>
                          <% } %>
                      </div>
                      <div class="p-4">
                        <h3 class="text-sm font-medium mb-1">Casen <%= productName %> (<%= color %>)</h3>
                        <p class="text-xs text-gray-500 mb-2">
                          <%= categoryName %>
                        </p>
                        <div class="flex items-center gap-2">
                          <% if (p.offerInfo) { %>
                            <span class="text-red-600 font-bold">Rs. <%= p.variants.price.toFixed(2) %></span>
                            <span class="text-gray-400 line-through text-xs">Rs. <%= originalPrice.toFixed(2) %></span>
                            <% } else { %>
                              <span class="text-red-600 font-bold">Rs. <%= priceNum.toFixed(2) %></span>
                              <% } %>
                        </div>
                      </div>
                    </div>
                  </a>
                  <% } %>
            </div>
            <!-- Pagination -->
            <% if (pagination && pagination.totalPages> 1) { %>
              <% function buildUrl(newParams) { var params={}; if (query) { for (var key in query) { if
                (Object.prototype.hasOwnProperty.call(query, key)) { var val=query[key]; if (val !==undefined && val
                !==null && val !=='' ) { params[key]=val; } } } } for (var key in newParams) { if
                (Object.prototype.hasOwnProperty.call(newParams, key)) { var val=newParams[key]; if (val !==undefined &&
                val !==null && val !=='' ) { params[key]=val; } else { delete params[key]; } } } var pairs=[]; for (var
                key in params) { if (Object.prototype.hasOwnProperty.call(params, key)) {
                pairs.push(encodeURIComponent(key) + '=' + encodeURIComponent(params[key])); } } return pairs.length ?
                pairs.join('&') : '' ; } %>
                <div class="flex justify-center mt-8 space-x-1">
                  <% if (pagination.hasPrev) { %>
                    <a href="?<%= buildUrl({ page: String(pagination.currentPage - 1) }) %>"
                      class="px-3 py-1 border rounded hover:bg-gray-100">Prev</a>
                    <% } %>
                      <% for (var pageNum=1; pageNum <=pagination.totalPages; pageNum++) { %>
                        <% if (pageNum===pagination.currentPage) { %>
                          <span class="px-3 py-1 bg-red-600 text-white rounded">
                            <%= pageNum %>
                          </span>
                          <% } else { %>
                            <a href="?<%= buildUrl({ page: String(pageNum) }) %>"
                              class="px-3 py-1 border rounded hover:bg-gray-100">
                              <%= pageNum %>
                            </a>
                            <% } %>
                              <% } %>
                                <% if (pagination.hasNext) { %>
                                  <a href="?<%= buildUrl({ page: String(pagination.currentPage + 1) }) %>"
                                    class="px-3 py-1 border rounded hover:bg-gray-100">Next</a>
                                  <% } %>
                </div>
                <% } %>
                  <% } %>
      </div>
    </div>
  </main>
  <!-- Footer -->
