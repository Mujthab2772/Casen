      <!-- Footer Links -->
      <div class="grid grid-cols-1 md:grid-cols-4 gap-8">
        <div>
          <h3 class="font-bold text-lg mb-4">Discover More</h3>
          <ul class="space-y-2 text-sm">
            <li><a href="#" class="hover:text-gray-300">About Us</a></li>
